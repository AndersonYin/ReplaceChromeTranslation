{
  "manifest_version": 3,
  "name": "ReplaceChromeTranslation",
  "description": "Base Level Extension",
  "version": "1.0",
  "action": {
    "default_icon": "logo.png"
  },
  "permissions": [
    "contextMenus",
    "sidePanel",
    "storage"
  ],
  "background":{
    "service_worker": "background.js",
    "openPanelOnActionClick": true
  },
  "side_panel": {
    "default_path": "sidebar.html"
  }
}